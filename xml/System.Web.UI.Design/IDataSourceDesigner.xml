    <altmember cref="T:System.ComponentModel.DesignerAttribute" />
    <related type="Article" href="https://docs.microsoft.com/previous-versions/visualstudio/visual-studio-2010/37899azc(v=vs.100)"><span data-ttu-id="8988e-111">扩展设计时支持</span><span class="sxs-lookup"><span data-stu-id="8988e-111">Extending Design-Time Support</span></span></related>
